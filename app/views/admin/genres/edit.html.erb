<div class='body'>
  <div class='container'>
    <div class="row content">
    	<h1>ジャンル編集</h1>
         <%= form_for(@genre, url: admin_genre_path) do |f| %>
        <tr>
	      <td>ジャンル</td>
	      <td><%= f.text_field :name %></td>
	      <td><%= f.radio_button :void_frag, :有効 %>有効</td>
	      <td><%= f.radio_button :void_frag, :無効 %>無効</td>
	      <td><%= f.submit "変更を保存する", class:"btn btn-primary" %></td>
        </tr>
       <% end %>
     </div>
   </div>
 </div>
